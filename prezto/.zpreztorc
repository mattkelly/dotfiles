# -*- mode: bash -*-
# vi: set ft=bash

# Prezto modules to load. Order matters.
zstyle ':prezto:load' pmodule \
    'environment' \
    'terminal' \
    'editor' \
    'directory' \
    'spectrum' \
    'utility' \
    'ssh' \
    'homebrew' \
    'osx' \
    'syntax-highlighting' \
    'prompt' # must be last

zstyle ':prezto:module:prompt' theme 'powerlevel10k'
